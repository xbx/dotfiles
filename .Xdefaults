Xcursor.theme: Jimmac
Xft*dpi:        96
Xft*antialias:  true

!URxvt settings
URxvt*termName:     xterm-color
URxvt*font:         xft:Terminus:pixelsize=12:antialias=true
!URxvt*font:         xft:DejaVu:pixelsize=11:antialias=true
!URxvt*font:         xft:Monaco:pixelsize=12

!urxvt.font: xft:DejaVu Sans Mono:pixelsize=12
!urxvt.letterSpace: -1

URxvt.imLocale:     en_US.UTF-8
URxvt*transparent:  true
URxvt*shading:      10
URxvt*scrollBar:    false
URxvt*geometry:     80x27
URxvt.cursorBlink:           True
URxvt*urlLauncher:           chromium
URxvt*perl-eXT-common:       selection-autotransform,matcher
URxvt*matcher.button:        2
!URxvt.tabbed.secondaryScroll: true
!URxvt.tabbed.urlLauncher:     chromium
!URxvt.tabbed.matcher.button:  2
!URxvt.letterSpace:   -1

URxvt.saveLines: 90000

!COLORS 

! terminal colors ------------------------------------------------------------

! tangoesque scheme
*background: #111111
*foreground: #bdbdbd
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #dc143c
! DarkGreen + Green
*color2:  #98fb98
*color10: #32cd32
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #1e90ff
! DarkMagenta + Magenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff
